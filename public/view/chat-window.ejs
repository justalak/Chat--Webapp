<body>
    <% include ./partial/header %>
    <% include ./partial/frame %>
    <div class="modal fade" id="addfrModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalCenterTitle">Add Friend</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="alert alert-danger" id="not-exist" role="alert">
                        User do not exist
                    </div>
                    <div class="alert alert-primary" id="add-before" role="alert">

                    </div>
                    <div class="alert alert-success" id="add-success" role="alert">

                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" id="addfr-input" placeholder="Email or username">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button class="btn btn-primary" id="addfr-btn">Add contact</button>
                </div>
            </div>
        </div>
    </div>
    <script src="../Script/lib/jquery-3.4.1.min.js" type="text/javascript"></script>
    <script src="../Script/lib/popper.min.js" type="text/javascript"></script>
    <script src="../Script/lib/bootstrap.js" type="text/javascript"></script>
